<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="lstyle" name="L Style" inherit_id="website.assets_frontend">
            <xpath expr="link[last()]" position="after">
                <link href="/theme_l/static/less/style.less" rel="stylesheet" type="text/less" />
            </xpath>
        </template>
        <template id="l_js" inherit_id="website.assets_editor" name="L Js">
            <xpath expr="script[last()]" position="after">
                <script src="/theme_l/static/src/js/tutorial_editor.js" type="text/javascript" />
            </xpath>
        </template>
        <template id="l_js" inherit_id="website.assets_frontend" name="L Js">
            <xpath expr="script[last()]" position="after">
                <script src="/theme_l/static/src/js/theme.js" type="text/javascript" />
            </xpath>
        </template>
    </data>
</odoo>

